			setReadOnlyDialogPreferences();
			suppress = false;
		} else if(systemPreferences.downloadAttachmentDialog.suppress == true) {
			suppress = true;
		}
	} else {
		suppress = false;
		setReadOnlyDialogPreferences();
	}

	return suppress;
}

